@article{Bopp1980,
  author={B. W. Bopp},
  title={HD 45088 as a BY DRA variable.},
  journal={Publications of the Astronomical Society of the Pacific},
  volume={92},
  number={546},
  pages={218},
  url={http://stacks.iop.org/1538-3873/92/i=546/a=218},
  year={1980},
  abstract={Spectroscopic observations of the secondary component of the binary system HD 45088 which suggest its characterization as a BY Draconis variable are reported. Coude spectrograms of the system in the blue are observed to show absorption features only from the primary star, with double reversals of the Ca II H and K lines of nearly equal intensities. Red image tube spectra obtained on a subsequent night showed the absorption lines of the cooler secondary near 6400 A, however the H alpha line appears as a single absorption feature. The presence of secondary absorption lines in the red but not in the blue is explained by attributing a spectral type of K5 to the secondary, implying intrinsically very strong Ca II emission, with the H alpha line filled by emission, as in the single BY Draconis variable EQ Vir. It is predicted that the secondary of HD 45088 should exhibit photometric variations with a period near the 6.99-day orbital period, and it is suggested that the primary may also turn out to be a BY Draconis variable.}
}
	
